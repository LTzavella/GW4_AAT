@Article{moshontzPsychologicalScienceAccelerator2018,
  title = {The {{Psychological Science Accelerator}}: {{Advancing Psychology Through}} a {{Distributed Collaborative Network}}},
  volume = {1},
  issn = {2515-2459},
  shorttitle = {The {{Psychological Science Accelerator}}},
  abstract = {Concerns about the veracity of psychological research have been growing. Many findings in psychological science are based on studies with insufficient statistical power and nonrepresentative samples, or may otherwise be limited to specific, ungeneralizable settings or populations. Crowdsourced research, a type of large-scale collaboration in which one or more research projects are conducted across multiple lab sites, offers a pragmatic solution to these and other current methodological challenges. The Psychological Science Accelerator (PSA) is a distributed network of laboratories designed to enable and support crowdsourced research projects. These projects can focus on novel research questions or replicate prior research in large, diverse samples. The PSA's mission is to accelerate the accumulation of reliable and generalizable evidence in psychological science. Here, we describe the background, structure, principles, procedures, benefits, and challenges of the PSA. In contrast to other crowdsourced research networks, the PSA is ongoing (as opposed to time limited), efficient (in that structures and principles are reused for different projects), decentralized, diverse (in both subjects and researchers), and inclusive (of proposals, contributions, and other relevant input from anyone inside or outside the network). The PSA and other approaches to crowdsourced psychological science will advance understanding of mental processes and behaviors by enabling rigorous research and systematic examination of its generalizability.},
  language = {en},
  number = {4},
  journal = {Advances in Methods and Practices in Psychological Science},
  doi = {10.1177/2515245918797607},
  author = {Hannah Moshontz and Lorne Campbell and Charles R. Ebersole and Hans IJzerman and Heather L. Urry and Patrick S. Forscher and Jon E. Grahe and Randy J. McCarthy and Erica D. Musser and Jan Antfolk and Christopher M. Castille and Thomas Rhys Evans and Susann Fiedler and Jessica Kay Flake and Diego A. Forero and Steve M. J. Janssen and Justin Robert Keene and John Protzko and Balazs Aczel and Sara {{\~A}?lvarez Solas} and Daniel Ansari and Dana Awlia and Ernest Baskin and Carlota Batres and Martha Lucia Borras-Guevara and Cameron Brick and Priyanka Chandel and Armand Chatard and William J. Chopik and David Clarance and Nicholas A. Coles and Katherine S. Corker and Barnaby James Wyld Dixson and Vilius Dranseika and Yarrow Dunham and Nicholas W. Fox and Gwendolyn Gardiner and S. Mason Garrison and Tripat Gill and Amanda C. Hahn and Bastian Jaeger and Pavol Ka{\v c}m{\a'a}r and Gwena{\~A}{\guillemotleft}l Kaminski and Philipp Kanske and Zoltan Kekecs and Melissa Kline and Monica A. Koehn and Pratibha Kujur and Carmel A. Levitan and Jeremy K. Miller and Ceylan Okan and Jerome Olsen and Oscar Oviedo-Trespalacios and Asil Ali {\~A}NANAzdo{\"A}{\"Y}ru and Babita Pande and Arti Parganiha and Noorshama Parveen and Gerit Pfuhl and Sraddha Pradhan and Ivan Ropovik and Nicholas O. Rule and Blair Saunders and Vidar Schei and Kathleen Schmidt and Margaret Messiah Singh and Miroslav Sirota and Crystal N. Steltenpohl and Stefan Stieger and Daniel Storage and Gavin Brent Sullivan and Anna Szabelska and Christian K. Tamnes and Miguel A. Vadillo and Jaroslava V. Valentova and Wolf Vanpaemel and Marco A. C. Varella and Evie Vergauwe and Mark Verschoor and Michelangelo Vianello and Martin Voracek and Glenn P. Williams and John Paul Wilson and Janis H. Zickfeld and Jack D. Arnal and Burak Aydin and Sau-Chin Chen and Lisa M. DeBruine and Ana Maria Fernandez and Kai T. Horstmann and Peder M. Isager and Benedict Jones and Aycan Kapucu and Hause Lin and Michael C. Mensink and Gorka 