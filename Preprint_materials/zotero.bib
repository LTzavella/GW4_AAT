  author = {Sindhu R. Johnson and George A. Tomlinson and Gillian A. Hawker and John T. Granton and Brian M. Feldman},
  month = {apr},
  year = {2010},
  keywords = {Bayesian,Belief elicitation,Bias,Priors,Reliability,Validity},
  pages = {355-369},
